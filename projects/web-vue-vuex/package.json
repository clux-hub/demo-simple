  "author": "clux-hub <clux_hub@163.com>",
  "license": "MIT",
  "private": "true",
  "engines": {
    "node": ">=11.0.0"
  },
  "workspaces": [
    "public"
  ],
  "browserslist": [
    "ie >= 10"
  ],
  "scripts": {
    "patchActions": "clux-patch-actions patch",
    "typeCheck": "tsc --build ./tests",
    "eslint": "cross-env NODE_ENV=production eslint --cache **/*.{js,ts,tsx,vue}",
    "stylelint": "cross-env NODE_ENV=production stylelint --cache **/*.{css,less,vue}",
    "jsonlint": "prettier --write **/*.json",
    "buildPolyfill": "cross-env NODE_ENV=production node build/buildPolyfill.js",
    "prebuild": "yarn eslint && yarn stylelint && yarn jsonlint && yarn typeCheck",
    "build": "cross-env NODE_ENV=production PROJ_ENV=local node build/build.js",
    "start": "cross-env NODE_ENV=development PROJ_ENV=local DEBUG=true node build/start.js",
    "runLocal": "node ./dist/local/start.js",
    "debug": "node --inspect=19248 --debug-brk"
  },
  "resolutions": {
    "@types/node": "~14.14.7",
    "@types/jest": "~26.0.15",
    "@types/express": "~4.17.9"
  },
  "dependencies": {
    "@clux/vue-web": "~0.0.15",
    "vue": "~3.0.0",
    "vuex": "next"
  },
  "devDependencies": {
    "@clux/dev-pkg": "~0.0.3",
    "@clux/dev-utils": "~0.0.3",
    "@clux/dev-webpack": "~0.0.11",
    "@types/express": "~4.17.9",
    "@types/jest": "~26.0.15",
    "postcss-px2rem": "~0.3.0",
    "mockjs": "~1.1.0",
    "jest": "~26.6.3",
    "less": "~3.12.2",
    "less-loader": "~7.1.0",
    "typescript": "~4.2.0"
  }
}
